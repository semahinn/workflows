{
    "name": "snr/workflows",
    "description": "Provides a set of customizable items (blocks) for composing custom workflows",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Snr\\Workflows\\": "src/"
        }
    },
	"autoload-dev": {
        "psr-4": {
            "Snr\\Workflows\\Tests\\": "tests",
			"Snr\\Plugin\\Tests\\": "vendor/snr/plugin/tests"
        }
    },
    "authors": [
        {
            "name": "Nikita Semakhin",
            "email": "semahinnikita@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.3",
        "symfony/event-dispatcher": "3.2.14",
        "snr/plugin": "^1.0",
		"snr/access_result": "^1.0"
    },
    "require-dev": {
        "symfony/dependency-injection": "3.2.14",
        "phpstan/phpstan": "^1.12",
        "phpunit/phpunit": "^9.6"
    }
}
